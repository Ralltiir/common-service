{
  "name": "ralltiir-application",
  "version": "4.5.15",
  "description": "A common service implement",
  "main": "./service.js",
  "scripts": {
    "postversion": "git push --tags",
    "lint": "fecs check --level 2",
    "test": "mocha test/utils/url.spec.js",
    "doc:install": "gitbook install ./docs",
