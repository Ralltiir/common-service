  },
  "homepage": "https://github.com/Ralltiir/ralltiir-application#readme",
  "devDependencies": {
    "amd-loader": "0.0.8",
    "apmjs": "^3.2.4",
    "chai": "^4.2.0",
    "fecs": "^1.4.1",
    "gitbook-cli": "^2.3.2",
    "mocha": "^5.2.0",
    "path": "^0.12.7",
    "ralltiir": "^2.13.3",
    "requirejs": "^2.3.6"
  },
  "amdDependencies": {
    "@searchfe/assert": "^1.0.0",
